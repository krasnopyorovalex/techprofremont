{% set r = app.request %}
<div class="auto__names">
    <ul>
        {% for item in data.autoModels %}
            <li>
                <a href="/{{ r.get('catalog') }}/{{ r.get('category') }}/{{ r.get('subcategory') ? r.get('subcategory') ~ '/' : '' }}{{ r.get('subsubcategory') ? r.get('subsubcategory') ~ '/' : '' }}auto-{{ data.alias }}/{{ item.alias }}">
                    {{ item.name }}
                </a>
            </li>
        {% endfor %}
    </ul>
</div>